{
  "name": "css-frameworks-ca",
  "version": "0.0.1",
  "description": "A starting point for the CSS Frameworks CA project",
  "scripts": {
    "test": "echo \"We will learn more about testing in the Workflow course\" && exit 1",
    "copyJS": "npx copyfiles -f node_modules/bootstrap/dist/js/bootstrap.bundle.min.js dist",
    "build": "sass src/scss:css && npm run copyJS",
    "watch": "sass --watch src/scss:css",
    "start": "npm run build && live-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoroffFEU/css-frameworks-ca.git"
  },
  "keywords": [
    "CSS",
    "CSS",
    "Frameworks",
    "Bootstrap",
    "SCSS",
    "SASS"
  ],
  "author": "me",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NoroffFEU/css-frameworks-ca/issues"
  },
  "homepage": "https://github.com/NoroffFEU/css-frameworks-ca#readme",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "live-server": "^1.2.2"
  },
  "devDependencies": {
    "sass": "^1.70.0"
  }
}
